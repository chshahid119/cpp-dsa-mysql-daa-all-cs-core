.PHONY: clean All

All:
	@echo "----------Building project:[ PassByReference - Debug ]----------"
	@cd "PassByReference" && "$(MAKE)" -f  "PassByReference.mk"
clean:
	@echo "----------Cleaning project:[ PassByReference - Debug ]----------"
	@cd "PassByReference" && "$(MAKE)" -f  "PassByReference.mk" clean
