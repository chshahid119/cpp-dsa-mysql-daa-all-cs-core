.PHONY: clean All

All:
	@echo "----------Building project:[ DefaultArguments - Debug ]----------"
	@cd "DefaultArguments" && "$(MAKE)" -f  "DefaultArguments.mk"
clean:
	@echo "----------Cleaning project:[ DefaultArguments - Debug ]----------"
	@cd "DefaultArguments" && "$(MAKE)" -f  "DefaultArguments.mk" clean
