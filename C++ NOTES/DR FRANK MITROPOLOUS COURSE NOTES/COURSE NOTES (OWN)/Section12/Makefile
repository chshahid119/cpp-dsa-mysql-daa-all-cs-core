.PHONY: clean All

All:
	@echo "----------Building project:[ PointersPracticeMySelf - Debug ]----------"
	@cd "PointersPracticeMySelf" && "$(MAKE)" -f  "PointersPracticeMySelf.mk"
clean:
	@echo "----------Cleaning project:[ PointersPracticeMySelf - Debug ]----------"
	@cd "PointersPracticeMySelf" && "$(MAKE)" -f  "PointersPracticeMySelf.mk" clean
